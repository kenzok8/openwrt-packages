{
    "luci-theme-kucat": {
        "description": "Grant UCI access for luci-theme-kucat",
        "read": {
            "uci": [ "kucat" ],
	    "file": {
                "/www/luci-static/kucat/img/": [ "read" ],
                "/www/luci-static/kucat/background/": [ "read" ],
                "/www/luci-static/resources/background/": [ "read" ],
                "/www/luci-static/kucat/": [ "list", "read" ]
      	    },
            "ubus": {
                "luci.kucat": [ "*" ]
            }
        },
        "write": {
            "ubus": {
                "luci.kucat": [ "*" ]
            },
	    "file": {
                "/www/luci-static/kucat/img/": [ "write" ],
                "/www/luci-static/kucat/background/": [ "write" ],
                "/www/luci-static/resources/background/": [ "write" ]
	    },
            "uci": ["kucat"]
        }
    }
}
