{
    "luci-app-partexp": {
        "description": "Grant UCI access for luci-app-partexp",
	"read": {
            "ubus": {
                        "file": ["exec", "list", "stat", "read"],
                        "uci": [ "*" ],
                        "partexp": ["*"]
            }
	},
	"write": {
            "ubus": {
                "partexp": ["*"],
                "file": ["write"],
                "uci": ["*"]
      }
    }
  }
}