{
    "luci-app-partexp": {
        "description": "Grant access for luci-app-partexp",
        "read": {
            "ubus": {
                "file": ["exec", "list", "stat", "read"],
                "uci": ["partexp"],
                "partexp": ["get_devices", "get_log", "get_status"]
            }
        },
        "write": {
            "ubus": {
                "partexp": ["autopart", "save_config"],
                "file": ["write"]
            },
            "file": {
                "/etc/config/partexp": ["write"]
            }
        }
    }
}